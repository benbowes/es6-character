{
  "parser": "babel-eslint",
  "rules": {
    "quotes": [1, "single"],
    "linebreak-style": [1, "unix"],
    "semi": [1, "always"],
    "no-console": [1],
    "comma-dangle": [2, "never"]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended"
}
